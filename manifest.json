{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "https://book.douban.com/*"
  ],
  "permissions": [
    "tabs",
    "alarms",
    "pageCapture",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  }
}